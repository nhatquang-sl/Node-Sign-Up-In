  "bugs": {
    "url": "https://github.com/nhatquang-sl/Node-Sign-Up-In/issues"
  },
  "homepage": "https://github.com/nhatquang-sl/Node-Sign-Up-In#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "node-mailjet": "^3.3.7",
    "sequelize": "^6.18.0",
    "tedious": "^14.4.0",
    "tsc": "^2.0.4",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
