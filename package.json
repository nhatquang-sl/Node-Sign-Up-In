    "@types/node-mailjet": "^3.3.8",
    "@types/tedious": "^4.0.7",
    "@types/uuid": "^8.3.4",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "14.x"
  },
  "_moduleAliases": {
    "@controllers": "build/controllers",
