{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development&& nodemon -r tsconfig-paths/register index.ts",
    "clean": "shx rm -rf public && shx rm -rf dist",
    "build-be": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && shx cp -R node-sign-up-in-credentials dist/back-end && shx mkdir dist/back-end/public",
    "build-fe": "cd ../front-end && npm run build && shx mv build/* ../back-end/dist/back-end/public && cd ../back-end",
    "build": "npm run clean && npm run build-be && npm run build-fe",
    "test": "set NODE_ENV=test&& jest",
    "test-watch": "set NODE_ENV=test&& jest --watchAll",
    "start": "npm run build-be && node dist/back-end/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@qnn92/mediatorts": "^1.0.7",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.194",
    "@types/node-mailjet": "^3.3.8",
    "@types/tedious": "^4.0.7",
    "@types/uuid": "^8.3.4",
    "axios": "^1.1.2",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "node-mailjet": "^3.3.13",
    "sequelize": "^6.19.0",
    "shx": "^0.3.4",
    "tedious": "^14.4.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.6.2",
